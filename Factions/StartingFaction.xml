  <AddIRCMessage Author="Yusa" TargetComp="sakuraHub" Delay="36.0">Oh, hello there. Welcome to the group, #PLAYERNAME#</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="39.0">(Did these people just totally ignore what I said?)</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="42.0">We should have #PLAYERNAME# go through a little initiation, don't you think?</AddIRCMessage>
  <AddIRCMessage Author="Yusa" TargetComp="sakuraHub" Delay="45.0">Yes, I agree.</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="48.0">Um... Initiation?</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="51.0">Ooh, yes! Let's do that!</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="55.0">Kuris-tina! Prepare the trials!</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="59.0">My name's not Kuris-tina! There's no "-tina!"</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="63.0">Come on, your name's just an anime reference. Let me make references of my own!</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="66.0">(Kuris-tina, huh? Kuris-tina... Christina. Hehe. I should use that.)</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="70.0">Jeez... Anyways.</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="73.0">What you have to do is...</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="76.0">Uh... Find the password to someone's ePhone!</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="80.0">You just came up with that on the spot, didn't you...</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="83.0">Shut up! Just find the password to someone's ePhone!</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="86.0">Is that it?</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="90.0">Got a problem with that?</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="93.0">No, no problem at all.</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="95.0">(Jeez, what's her deal?)</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="99.0">Good. I've put the link and stuff on the mission listing.</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="102.0">(The what now?)</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="105.0">Don't disappoint me.</AddIRCMessage>
  <AddIRCMessage Author="#PLAYERNAME#" TargetComp="sakuraHub" Delay="108.0">Got it... Christina.</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="111.0">...eh?</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="115.0">Christina. Hahaha!</AddIRCMessage>
  <AddIRCMessage Author="Yusa" TargetComp="sakuraHub" Delay="119.0">Good luck, @#PLAYERNAME#</AddIRCMessage>
  <AddIRCMessage Author="Kurisu" TargetComp="sakuraHub" Delay="124.0">Wait, I'm Christina!?</AddIRCMessage>
  <AddIRCMessage Author="Coel" TargetComp="sakuraHub" Delay="128.0">Took you long enough.</AddIRCMessage>
</Action>

<!--After the initiation-->
